                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT prodm.IDPRODUCTO AS IDPRODUCTO,
perdm.IDCLIENTE AS IDPERSONA,
provdm.IDPROVEEDOR AS IDPROVEEDOR,
tiedm.IDFECHA AS IDTIEMPO,
odeta.UnitPrice AS PRECIO,
odeta.OrderQty AS CANTIDAD
FROM AdventureWorks2014.Production.Product pro
INNER JOIN AdventureWorks2014.Production.ProductSubcategory scat ON (pro.ProductSubcategoryID=scat.ProductSubcategoryID)
INNER JOIN AdventureWorks2014.Production.ProductCategory cat ON (scat.ProductCategoryID=cat.ProductCategoryID)
INNER JOIN AdventureWorks2014.Purchasing.ProductVendor pven ON (pro.ProductID=pven.ProductID)
INNER JOIN AdventureWorks2014.Purchasing.Vendor ven ON (pven.BusinessEntityID=ven.BusinessEntityID)
INNER JOIN AdventureWorks2014.Sales.SalesOrderDetail odeta ON (pro.ProductID=odeta.ProductID)
INNER JOIN AdventureWorks2014.Sales.SalesOrderHeader oheader ON (odeta.SalesOrderID=oheader.SalesOrderID)
INNER JOIN AdventureWorks2014.sales.Customer cus ON (oheader.CustomerID=cus.CustomerID)
INNER JOIN AdventureWorks2014.Person.Person per ON (cus.PersonID=per.BusinessEntityID)
INNER JOIN DMAdventureWorks2014.dbo.DIM_PRODUCTO prodm ON (prodm.NOMBRE COLLATE Modern_Spanish_CI_AS = pro.Name COLLATE Modern_Spanish_CI_AS)
INNER JOIN DMAdventureWorks2014.dbo.DIM_CLIENTE perdm ON (perdm.NOMBRE COLLATE Modern_Spanish_CI_AS = (per.FirstName+' '+per.LastName) COLLATE Modern_Spanish_CI_AS)
INNER JOIN DMAdventureWorks2014.dbo.DIM_PROVEEDOR provdm ON (provdm.NOMBRE COLLATE Modern_Spanish_CI_AS= ven.Name COLLATE Modern_Spanish_CI_AS)
INNER JOIN DMAdventureWorks2014.dbo.DIM_TIEMPO tiedm ON (tiedm.FECHA = CONVERT(DATE,oheader.OrderDate))</property>
